$('body').click(function(e){
    var Elem = e.target;
    if (Elem.id == 'commentaire'){
        bootbox.prompt("Commentaire :", function(result) {
        if (result !== null) {                                             
            var requete = $.ajax({
                url: "{{ path('mgateSuivi_state') }}",
                type: "POST",
                data: "state=" + result +"&etude=" + $(Elem).data("title") + "&id=" + $(Elem).data("content"),
                success: function(){},
                });
            }
        })
    }
});