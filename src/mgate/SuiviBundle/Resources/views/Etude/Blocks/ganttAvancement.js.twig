$(".contain").css({'margin' : '0 auto', 'width' : $(document).width() - 200 + 'px'});
		$(function() {

			"use strict";
                        
			$("#ganttId{{id}}").gantt({
				source: [
                                {% if etude.dateCreation %}   
                                {
					name: "Etude",
					desc: "Création",
					values: [{ title : "Création de l'Etude",
                                                content : "Créée le {{etude.dateCreation|date('d/m/Y')}}",
						from: "/Date({{etude.dateCreation|date('U') - 86400}}000)/",
						to: "/Date({{etude.dateCreation|date('U') - 86400}}000)/",
						label: "Start", 
						customClass: "ganttBlue",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% endif %}
                                {% if getDateLancement(etude) %}   
                                {
					name: "",
					desc: "Durée",
					values: [{ title : "Durée", content : "Lancée le terminé le",
						from: "/Date({{getDateLancement(etude)|date('U') - 86400}}000)/",
						to: "/Date({{getDateFin(etude)|date('U') - 86400}}000)/",
						label: "Durée de l'Etude", 
						customClass: "ganttGreen",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% endif %}
                                {% if etude.ap and etude.ap.datesignature %}  
                                {
					name: "Avant-Projet",
					desc: "Signé",
					values: [{ title : "blabla", content : "fdsfds",
						from: "/Date({{etude.ap.datesignature|date('U') - 86400}}000)/",
						to: "/Date({{etude.ap.datesignature|date('U') - 86400}}000)/",
						label: "AP", 
						customClass: "ganttGreen",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% else %}{#else if verifierDate == false affiche Erreur ... gantt RED#}
                                {
					name: "Avant-Projet",
					desc: "Non Signé",
					values: [{ title : "fdsfds", content : "fdsfds",
						from: "/Date({{"now"|date('U') - 86400}}000)/",
						to: "/Date({{"now"|date('U') - 86400}}000)/",
						label: "AP", 
						customClass: "ganttRed",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% endif %}
                                {% if etude.cc and etude.cc.datesignature %}  
                                {
					name: "Convention Client",
					desc: "Signé",
					values: [{ title : "fdsfds", content : "fdsfds",
						from: "/Date({{etude.cc.datesignature|date('U') - 86400}}000)/",
						to: "/Date({{etude.cc.datesignature|date('U') - 86400}}000)/",
						label: "CC", 
						customClass: "ganttGreen",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% else %}
                                {
					name: "Convention Client",
					desc: "Non Signé",
					values: [{ title : "fdsfds", content : "fdsfds",
						from: "/Date({{"now"|date('U') - 86400}}000)/",
						to: "/Date({{"now"|date('U') - 86400}}000)/",
						label: "CC", 
						customClass: "ganttRed",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% endif %}
                                {% for clientContact in etude.clientcontacts %}   
                                {
					name: {%if loop.index == 1 %}"Contact Client" {%else%} "" {% endif%},
					desc: "Contact {{loop.index}}",
					values: [{ title : "", content : "",
						from: "/Date({{clientContact.date|date('U') - 86400}}000)/",
						to: "/Date({{clientContact.date|date('U') - 86400}}000)/",
						label: "C{{loop.index}}", 
						customClass: "ganttBlue",
                                                dataObj: "Durée de l'étude"
					}]
				},
                                {% endfor %}
                                {% for suivi in etude.suivis %}
                                {
					name: {%if loop.index == 1 %}"Suivi client" {%else%} "" {% endif%},
					desc: "Event {{loop.index}}",
					values: [{ title : "Suivi client n°{{suivi.id}}",
                                                content : "{{suivi.contenu}}",
						from: "/Date({{suivi.date.date|date('U')  - 86400}}000)/",
						to: "/Date({{suivi.date.date|date('U') - 86400}}000)/",
						label: "S{{suivi.id}}", 
						customClass: "ganttBlue",
                                                dataObj: "{{ path('mgateSuivi_suivi_modifier', {'id': suivi.id}) }}"
					}]
				},
                                {% endfor %}
                        ],
                                navigate: "scroll",
				scale: "weeks",
				maxScale: "months",
				minScale: "hours",
				itemsPerPage: 10,
				onItemClick: function(data) {
					document.location.href= data;
				},
				onAddClick: function(dt, rowId) {
					alert("Empty space clicked - add an item!");
				},
                                onRender: function() {
					if (window.console && typeof console.log === "function") {
						console.log("chart rendered");
					}
				}
			});

                  $(".gantt").popover({
				selector: ".bar",
				trigger: "hover",
                                placement: "top"
			});
			prettyPrint();

		});
