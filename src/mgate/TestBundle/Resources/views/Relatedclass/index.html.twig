            <td><a href="{{ path('Relatedclass_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.champ1 }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('Relatedclass_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('Relatedclass_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
