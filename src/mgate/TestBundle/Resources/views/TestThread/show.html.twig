<!DOCTYPE html>
 <html lang="en">
 <head>
   <meta charset="utf-8">
   <title>jQuery demo</title>
 </head>
 <body>
     
        <a href="http://jquery.com/">jQuery</a>
   <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script>


<h1>TestThread</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Permalink</th>
            <td>{{ entity.permalink }}</td>
        </tr>
        <tr>
            <th>Iscommentable</th>
            <td>{{ entity.isCommentable }}</td>
        </tr>
        <tr>
            <th>Numcomments</th>
            <td>{{ entity.numComments }}</td>
        </tr>
        <tr>
            <th>Lastcommentat</th>
            <td>{{ entity.lastCommentAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Auteur</th>
            <td>{{ entity.auteur }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('testthread') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('testthread_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('testthread_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>


{% include 'FOSCommentBundle:Thread:async.html.twig' with {'id': entity.id} %}



</body>

 </html>